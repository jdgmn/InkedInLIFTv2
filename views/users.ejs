<%- include('layout', { body: `
<h1>Users</h1>
<table class="table">
    <thead>
        <tr><th>Name</th><th>Email</th><th>Role</th></tr>
    </thead>
    <tbody>
        ${users.map(user => `
            <tr>
                <td>${user.firstName || ''} ${user.lastName || ''}</td>
                <td>${user.email}</td>
                <td>${user.role}</td>
            </tr>
        `).join('')}
    </tbody>
</table>
`}) %>