<%- include('layout', { body: `
  <h2>Check-ins</h2>
  <table class="table table-striped mt-3">
    <thead>
      <tr>
        <th>Member</th>
        <th>Date</th>
      </tr>
    </thead>
    <tbody>
      ${checkins.map(c => `
        <tr>
          <td>${c.user?.firstName || c.name || ''} ${c.user?.lastName || ''}</td>
          <td>${c.checkinTime ? new Date(c.checkinTime).toLocaleString() : ''}</td>
        </tr>
      `).join('')}
    </tbody>
  </table>
`}) %>
